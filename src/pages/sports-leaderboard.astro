---
import Header from "../components/Header.astro";
import Footer from "../components/Footer.astro";
import CommonMetaTags from "../components/CommonMetaTags.astro";

const pageUrl = "https://stjosephchurchairoli.in/sports-leaderboard/";
const pageTitle = "Sports Leaderboard - St. Joseph's Church, Airoli";
---

<!doctype html>
<html lang="en" class="no-js">
  <head>
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta charset="UTF-8" />
    <!-- Facebook Meta Tags -->
    <meta property="og:url" content={pageUrl} />
    <meta property="og:title" content={pageTitle} />

    <!-- Twitter Meta Tags -->
    <meta property="twitter:url" content={pageUrl} />
    <meta name="twitter:title" content={pageTitle} />

    <title>{pageTitle}</title>
    <CommonMetaTags />
  </head>

  <body>
    <Header />

    <!-- start banner Area -->
    <section class="banner-area relative about-banner" id="home">
      <div class="overlay overlay-bg"></div>
      <div class="container">
        <div class="row d-flex align-items-center justify-content-center">
          <div class="about-content col-lg-12">
            <h1 class="text-white">Sports Leaderboard 2023</h1>
            <p class="text-white link-nav">
              <a href="index.html">Home</a>
              <span class="fa fa-chevron-right"></span>
              <a href="sports-leaderboard.html"> Sports Leaderboard</a>
            </p>
          </div>
        </div>
      </div>
    </section>
    <!-- End banner Area -->

    <!-- Start contact-page Area -->
    <section class="contact-page-area section-gap">
      <div class="container-fluid">
        <div class="row">
          <div class="col-12" style="display: flex;">
            <iframe
              height="300"
              width="400"
              style="margin: auto;"
              src="https://docs.google.com/spreadsheets/d/e/2PACX-1vQDGPgmtR9WD5BiTZGoJL2fZy-jFdwvnhYuidreuEpQHU84yPIRKXTryRSfYN_52fcDdz6HgcRv9sGM/pubhtml?gid=1681621693&single=true&widget=true&headers=false"
            ></iframe>
          </div>
        </div>
        <div class="row justify-content-center mt-5">
          <div class="col-lg-10 col-md-11 col-12">
            <iframe
              class="results-sheet"
              style="width: 100%;margin: auto;height: 800px;"
              src="https://docs.google.com/spreadsheets/d/e/2PACX-1vQDGPgmtR9WD5BiTZGoJL2fZy-jFdwvnhYuidreuEpQHU84yPIRKXTryRSfYN_52fcDdz6HgcRv9sGM/pubhtml?gid=791871385&single=true&widget=true&headers=false"
            ></iframe>
          </div>
        </div>
      </div>
    </section>
    <!-- End contact-page Area -->

    <Footer />
  </body>
</html>
